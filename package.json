{
  "name": "zimmer-icons",
  "version": "1.0.3",
  "description": "Icon components + demo site",
  "main": "dist/zimmer-icons.cjs.js",
  "module": "dist/zimmer-icons.es.js",
  "types": "dist/index.d.ts",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/zimmer-icons.es.js",
      "require": "./dist/zimmer-icons.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.lib.config.ts && tsc --emitDeclarationOnly",
    "build:site": "vite build --outDir site-dist",
    "preview": "vite preview",
    "preview:site": "vite preview --outDir site-dist"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.5",
    "typescript": "^5.7.3"
  }
}
